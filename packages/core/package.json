{
  "name": "@bedrock-ui/core",
  "version": "0.0.5",
  "description": "Bedrock UI Core",
  "files": [
    "css",
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "pnpm build:css && pnpm build:ts",
    "build:css": "sass ./src/css/bedrock-ui.scss css/bedrock-ui.css --style compressed --no-source-map",
    "build:ts": "tsc && tsc-alias",
    "build-storybook": "build-storybook",
    "css-size": "node ./scripts/css-size.mjs",
    "dev": "concurrently \"npm run sass:watch\" \"npm run storybook\"",
    "storybook": "start-storybook -p 6006",
    "typecheck": "tsc --noEmit",
    "sass:watch": "sass --watch ./src/css/bedrock-ui.scss css/bedrock-ui.css --style compressed"
  },
  "author": "Matthew Wolfe",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "babel-loader": "^8.2.5",
    "bytes": "^3.1.2",
    "concurrently": "^7.3.0",
    "gzip-size": "^7.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.53.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
